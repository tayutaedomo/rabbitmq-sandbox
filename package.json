{
  "name": "rabbitmq-sandbox",
  "version": "0.1.0",
  "description": "Try RabbitMQ with Python",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "run:rabbitmq": "docker run -d --name rabbitmq -p 5672:5672 -p 15672:15672 rabbitmq:3-management",
    "compose:up": "docker-compose -f docker/docker-compose.yml up -d",
    "compose:down": "docker-compose -f docker/docker-compose.yml down",
    "open:admin": "open http://localhost:15672",
    "publish:hello": "poetry run python rabbitmq_sandbox/publish_hello.py",
    "subscribe:hello": "poetry run python rabbitmq_sandbox/subscribe_hello.py",
    "publish:auto_act": "poetry run python rabbitmq_sandbox/publish_auto_act.py",
    "subscribe:auto_act": "poetry run python rabbitmq_sandbox/subscribe_auto_act.py",
    "subscribe:auto_act:2": "poetry run python rabbitmq_sandbox/subscribe_auto_act_requeue.py",
    "test": "poetry run pytest",
    "test:mq": "poetry run pytest tests/test_rabbitmq.py",
    "test:mock": "poetry run pytest tests/test_rabbitmq_mock.py"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
